<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping>
	<class name="com.jcommerce.core.model.AccountLog" table="account_log">
		<id name="id" column="log_id" type="string" length="32">
			<generator class="uuid.hex" />
		</id>

		<property name="userMoney" column="user_money" />
		<property name="frozenMoney" column="frozen_money" />
		<property name="rankPoints" column="rank_points" />
		<property name="payPoints" column="pay_points" />
		<property name="changeTime" column="change_time" />
		<property name="changeDescription" column="change_desc" length="255" />

		<many-to-one name="user" column="user_id" class="com.jcommerce.core.model.User" cascade="none" />

	</class>
</hibernate-mapping>