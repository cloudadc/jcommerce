<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping>
 <class name="com.jcommerce.core.model.Pack" table="tis_pack" >
  <id name="id" column="pack_id" type="string" length="32">
   <generator class="uuid.hex" />
  </id>

  <property name="name" column="pack_name" length="120"/>
  <property name="image" column="pack_img" length="255"/>
  <property name="fee" column="pack_fee" />
  <property name="freeMoney" column="free_money"/>
  <property name="description" column="pack_desc" length="255"/>

 </class>
</hibernate-mapping>